<div class="container">
<div class="feed">
	<h1>Personal Feed</h1>	
	<% @user.feed.each do |post| %>
	<div class="panel panel-default">
		<div class="panel-body">
			<h3><a href="/posts/<%= post.id %>"><%= post.title %></a></h3>
			<p><%= post.user.username %></p>
		</div>
	</div>
	<% end %>
  </div>
</div>